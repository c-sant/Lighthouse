@{
    ViewData["Title"] = "Início";
}

<div class="body-custom">
    <div class="body-retangle typewriter">
        <p id="body-title"> LIGHTHOUSE</p>
        <p id="text">Nasceu, assim, o projeto Lighthouse, que, assim como um farol que guia navios durante a noite, pretende guiar vítimas a um futuro mais seguro</p>
    </div>
</div>

<script>
    var i = 0;
// var A = 0;
    var tag = document.getElementById("text");
    var html = document.getElementById("text").innerHTML;
    var attr = tag.setAttribute("data", html);
    var txt = tag.getAttribute("data");
    var speed = 270;

    function typeWriter() {
      if (i <= txt.length) {
        document.getElementById("text").innerHTML = txt.slice(0 , i + 1);
        i++;
        setTimeout(typeWriter, speed);
      }
        console.log(document.getElementById("text").innerHTML);
    }

       typeWriter();
</script>





